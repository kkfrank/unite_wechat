<view wx:for="{{sponsors}}" wx:for-item="item" wx:key="id" class='sponsors'>
  <navigator url="../detail/detail?id={{item.id}}" class='sponsors-item p20'>
    <image src="{{item.src}}" style="width:50px;height:50px"></image>
    <view class='sponsors-item-info ml10'>
      <view class=''>{{item.name}}</view>
      <view>{{item.desc}}</view>
    </view>
   </navigator>
</view>
